/*QScrollBar*/
QScrollBar {
	background: #dae2eb;
	border: 1px solid #ffffff;
}
QScrollBar:horizontal {
	height: 16px;
	padding: 0px 16px 0px 16px;
}
QScrollBar:vertical {
	width: 16px;
	padding: 16px 0px 16px 0px;
}
QScrollBar::handle {
	min-width: 12px;
	min-height: 12px;
	background: #738da3;
	border: 1px solid #738da3;
	border-radius: 7px;
}
QScrollBar::sub-line,
QScrollBar::add-line {
	background-color: transparent;
	width: 16px;
	height: 16px;
}
QScrollBar::sub-line:horizontal {
    subcontrol-position: left;
}
QScrollBar::up-arrow {
	image: url(:/res/ScrollBar_up.png);
}
QScrollBar::left-arrow {
	image: url(:/res/ScrollBar_left.png);
}
QScrollBar::down-arrow {
	image: url(:/res/ScrollBar_down.png);
}
QScrollBar::right-arrow {
	image: url(:/res/ScrollBar_right.png);
}
/*QScrollArea样式*/
QScrollArea {
	border: 1px solid #cad7e3;
